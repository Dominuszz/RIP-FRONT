{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "RIP-FRONT",
  "version": "0.1.0",
  "identifier": "com.RIP-FRONT.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "https://172.29.128.1:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "tsc && vite build --mode tauri"
  },
  "app": {
    "windows": [
      {
        "title": "RIP-FRONT",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,

        "devtools": true

      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "plugins": {
    "http": {
      "enabled": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}